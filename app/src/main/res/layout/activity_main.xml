<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <Button
            android:id="@+id/openBusinessOne"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="openBusinessOne"/>
        <Button
            android:id="@+id/openUserActivity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="open UserActivity" />
        <Button
            android:id="@+id/openFragmentActivity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="open FragmentActivity" />
        <Button
            android:id="@+id/openWebViewActivity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="open WebViewActivity" />
        <Button
            android:id="@+id/openHandlerActivity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="open HandlerActivity" />
        <Button
            android:id="@+id/openEventActivity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="open EventActivity" />
        <Button
            android:id="@+id/openServiceActivity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="open ServiceTestActivity"/>
        <Button
            android:id="@+id/openAnimationActivity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="open AnimationActivity"/>
        <Button
            android:id="@+id/openCustomViewActivity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="open CustomView Activity"/>
        <Button
            android:id="@+id/openReactActivity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="open React Activity"/>
        <Button
            android:id="@+id/openSurfaceActivity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="openSurfaceActivity" />
        <Button
            android:id="@+id/openVideoFullScreenActivity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="openVideoFullScreenActivity" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>